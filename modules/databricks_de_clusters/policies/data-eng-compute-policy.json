{
  "aws_attributes.availability": {
    "type": "fixed",
    "value": "SPOT_WITH_FALLBACK",
    "hidden": true
  },
  "aws_attributes.zone_id": {
    "type": "unlimited",
    "defaultValue": "auto",
    "hidden": true
  },
  "autotermination_minutes": {
    "type": "unlimited",
    "defaultValue": 60,
    "hidden": true
  },
  "runtime_engine": {
    "type": "fixed",
    "value": "STANDARD",
    "hidden": true
  },
  "node_type_id": {
    "type": "allowlist",
    "values": [
      "m5d.large",
      "m5d.xlarge",
      "m5d.2xlarge",
      "m5d.4xlarge",
      "i3.large",
      "i3.xlarge",
      "i3.2xlarge",
      "i3.4xlarge",
      "r5d.large",
      "r5d.xlarge",
      "r5d.2xlarge",
      "r5d.4xlarge",
      "c6id.large",
      "c6id.xlarge",
      "c6id.2xlarge",
      "c6id.4xlarge"
    ],
    "defaultValue": "m5d.large"
  },
  "num_workers": {
    "type": "unlimited",
    "defaultValue": 3,
    "isOptional": true
  },
  "spark_version": {
    "type": "unlimited",
    "defaultValue": "auto:latest-lts"
  },
  "cluster_type": {
    "type": "unlimited",
    "defaultValue": "all-purpose"
  },
  "enable_elastic_disk": {
    "defaultValue": true,
    "type": "unlimited",
    "hidden": true
  },
  "dbus_per_hour": {
    "type": "range",
    "maxValue": 10
  }
}
